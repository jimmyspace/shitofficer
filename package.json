{
  "name": "fengke_pc_ssr",
  "description": "fengke_pc_ssr 升级服务端渲染",
  "author": "Jimmy",
  "private": false,
  "scripts": {
    "dev": "node server",
    "test": "cross-env NODE_ENV=test node server",
    "start": "cross-env NODE_ENV=production node server",
    "build": "rimraf dist && npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.client.config.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.config.js --progress --hide-modules",
    "test_build": "rimraf dist && npm run test_build:client && npm run test_build:server",
    "test_build:client": "cross-env NODE_ENV=test webpack --config build/webpack.client.config.js --progress --hide-modules",
    "test_build:server": "cross-env NODE_ENV=test webpack --config build/webpack.server.config.js --progress --hide-modules"
  },
  "engines": {
    "node": ">=7.0",
    "npm": ">=4.0"
  },
  "dependencies": {
    "particles.js": "^2.0.0",
    "axios": "^0.18.0",
    "compression": "^1.7.1",
    "cookie-parser": "^1.4.3",
    "cross-env": "^5.1.1",
    "es6-promise": "^4.1.1",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "http-proxy-middleware": "^0.17.4",
    "lru-cache": "^4.1.1",
    "route-cache": "0.4.3",
    "serve-favicon": "^2.4.5",
    "vue": "^2.5.16",
    "vue-awesome-swiper": "^3.1.3",
    "vue-quill-editor": "^3.0.6",
    "vue-router": "^3.0.1",
    "vue-server-renderer": "^2.5.16",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0",
    "@tweenjs/tween.js": "^17.1.0",
    "url-search-params-polyfill": "^4.0.0",
    "babel-polyfill": "^6.26.0"
  },
  "devDependencies": {
    "axios": "^0.18.0",
    "autoprefixer": "^7.1.6",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.6.1",
    "chokidar": "^1.7.0",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.5",
