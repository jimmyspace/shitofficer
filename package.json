    "webpack": "^3.8.1",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.20.0",
    "webpack-merge": "^4.1.1",
    "webpack-node-externals": "^1.6.0",
    "http-proxy-middleware": "^0.17.4",
    "@tweenjs/tween.js": "^17.1.0",
    "babel-polyfill": "^6.26.0"
  }
